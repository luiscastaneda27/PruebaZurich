    <label>Seguros Colectivos</label>
    <oauthConfig>
        <callbackUrl>https://test.salesforce.com/oAuth2/callback</callbackUrl>
        <consumerKey></consumerKey>
        <isAdminApproved>true</isAdminApproved>
